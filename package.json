{
  "name": "bumblebee",
  "version": "1.0.0",
  "description": "Run `npm run tsc` for the typescript compiler to watch for file changes.\r Run `nodemon dist/server.js` for nodemon to (re)start the API on file changes.",
  "main": "dist/server.js",
  "scripts": {
    "watch": "tsc --watch",
    "debug": "nodemon --watch ./dist --inspect=0.0.0.0:5858 ./dist/server.js",
    "compile": "tsc",
    "start-smart": "if test \"$NODE_ENV\" = \"production\" ; then npm run start ; else npm run start-dev;  fi ",
    "start-dev": "nodemon  --delay 150ms -L --exec \"npm run compile && node --inspect=0.0.0.0:5858 ./dist/server.js\"",
    "start": "node ./dist/server.js"
  },
  "keywords": [],
  "author": "Sander Brokelman",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.8",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/guid": "^1.0.0",
    "@types/lodash": "^4.14.161",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^11.15.21",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.3",
    "@types/q": "^1.5.4",
    "@types/request": "^2.48.5",
    "@types/validator": "^13.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "guid": "0.0.12",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.2",
    "passport": "^0.4.1",
    "passport-jwt": "^3.0.1",
    "q": "^1.5.1",
    "request": "^2.88.2",
    "validator": "^13.1.1",
    "ytdl-core": "^2.1.7"
  },
  "devDependencies": {
    "typescript": "^4.0.2"
  }
}
